<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/presetsFragment">

    <fragment
        android:id="@+id/editCategoriesFragment"
        android:name="com.switch2go.aac.settings.EditCategoriesFragment"
        android:label="EditCategoriesFragment"
        tools:layout="@layout/fragment_edit_categories">
        <action
            android:id="@+id/action_editCategoriesFragment_to_editCategoriesKeyboardFragment"
            app:destination="@id/editCategoriesKeyboardFragment" />
        <action
            android:id="@+id/action_editCategoriesFragment_to_editCategoryMenuFragment"
            app:destination="@id/editCategoryMenuFragment" />

    </fragment>
    <fragment
        android:id="@+id/editCategoriesKeyboardFragment"
        android:name="com.switch2go.aac.settings.EditCategoriesKeyboardFragment"
        android:label="EditCategoriesKeyboardFragment"
        tools:layout="@layout/fragment_edit_keyboard">
        <argument
            android:name="category"
            app:argType="com.switch2go.aac.presets.Category"
            app:nullable="true"
            android:defaultValue="@null" />
        <action
            android:id="@+id/action_editCategoriesKeyboardFragment_to_editCategoryMenuFragment"
            app:destination="@id/editCategoryMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/editCategoryPhrasesFragment"
        android:name="com.switch2go.aac.settings.EditCategoryPhrasesFragment"
        android:label="EditCategoryPhrasesFragment">
        <argument
            android:name="category"
            app:argType="com.switch2go.aac.presets.Category" />
        <action
            android:id="@+id/action_editCategoryPhrasesFragment_to_editCategoriesKeyboardFragment"
            app:destination="@id/editCategoriesKeyboardFragment" />
        <action
            android:id="@+id/action_editCategoryPhrasesFragment_to_addPhraseKeyboardFragment"
            app:destination="@id/addPhraseKeyboardFragment" />
        <action
            android:id="@+id/action_editCategoryPhrasesFragment_to_editPhrasesKeyboardFragment"
            app:destination="@id/editPhrasesKeyboardFragment" />
        <action
            android:id="@+id/action_editCategoryPhrasesFragment_to_phraseEditMenuFragment"
            app:destination="@id/phraseEditMenuFragment" />
    </fragment>
    <fragment
        android:id="@+id/phraseEditMenuFragment"
        android:name="com.switch2go.aac.settings.PhraseEditMenuFragment"
        android:label="PhraseEditMenuFragment"
        tools:layout="@layout/fragment_phrase_edit_menu">
        <argument
            android:name="phrase"
            app:argType="com.switch2go.aac.presets.Phrase" />
        <argument
            android:name="category"
            app:argType="com.switch2go.aac.presets.Category" />
        <action
            android:id="@+id/action_phraseEditMenuFragment_to_editPhrasesKeyboardFragment"
            app:destination="@id/editPhrasesKeyboardFragment" />
        <action
            android:id="@+id/action_phraseEditMenuFragment_to_phraseStyleEditorFragment"
            app:destination="@id/phraseStyleEditorFragment" />
    </fragment>
    <fragment
        android:id="@+id/phraseStyleEditorFragment"
        android:name="com.switch2go.aac.settings.PhraseStyleEditorFragment"
        android:label="PhraseStyleEditorFragment"
        tools:layout="@layout/fragment_phrase_style_editor">
        <argument
            android:name="phrase"
            app:argType="com.switch2go.aac.presets.Phrase" />
    </fragment>
    <fragment
        android:id="@+id/sensitivityFragment"
        android:name="com.switch2go.aac.settings.SensitivityFragment"
        android:label="SensitivityFragment"
        tools:layout="@layout/fragment_timing_sensitivity" />
    <fragment
        android:id="@+id/selectionModeFragment"
        android:name="com.switch2go.aac.settings.selectionmode.SelectionModeFragment"
        android:label="SelectionModeFragment"
        tools:layout="@layout/fragment_selection_mode" />
    <fragment
        android:id="@+id/editPhrasesKeyboardFragment"
        android:name="com.switch2go.aac.settings.EditPhrasesKeyboardFragment"
        android:label="EditPhrasesKeyboardFragment"
        tools:layout="@layout/fragment_edit_keyboard">
        <argument
            android:name="phrase"
            app:argType="com.switch2go.aac.presets.Phrase" />
    </fragment>
    <fragment
        android:id="@+id/addPhraseKeyboardFragment"
        android:name="com.switch2go.aac.settings.AddPhraseKeyboardFragment"
        android:label="AddPhraseKeyboardFragment">
        <argument
            android:name="category"
            app:argType="com.switch2go.aac.presets.Category" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.switch2go.aac.settings.SettingsFragment"
        android:label="SettingsFragment"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settingsFragment_to_sensitivityFragment"
            app:destination="@id/sensitivityFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_selectionModeFragment"
            app:destination="@id/selectionModeFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_categoriesDisplaySettingsFragment"
            app:destination="@id/categoriesDisplaySettingsFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_advancedEyeTrackingFragment"
            app:destination="@id/advancedEyeTrackingFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_privacyPolicyFragment"
            app:destination="@id/privacyPolicyFragment" />
    </fragment>
    <fragment
        android:id="@+id/privacyPolicyFragment"
        android:name="com.switch2go.aac.settings.PrivacyPolicyFragment"
        android:label="PrivacyPolicyFragment"
        tools:layout="@layout/fragment_privacy_policy" />
    <fragment
        android:id="@+id/categoriesDisplaySettingsFragment"
        android:name="com.switch2go.aac.settings.CategoriesDisplaySettingsFragment"
        android:label="CategoriesDisplaySettingsFragment"
        tools:layout="@layout/fragment_categories_display_settings">
        <action
            android:id="@+id/action_categoriesDisplaySettingsFragment_to_editCategoriesFragment"
            app:destination="@id/editCategoriesFragment" />
        <action
            android:id="@+id/action_categoriesDisplaySettingsFragment_to_cviDisplaySettingsFragment"
            app:destination="@id/cviDisplaySettingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/advancedEyeTrackingFragment"
        android:name="com.switch2go.aac.settings.AdvancedEyeTrackingFragment"
        android:label="AdvancedEyeTrackingFragment"
        tools:layout="@layout/fragment_advanced_eye_tracking" />
    <fragment
        android:id="@+id/cviDisplaySettingsFragment"
        android:name="com.switch2go.aac.settings.CVIDisplaySettingsFragment"
        android:label="CVIDisplaySettingsFragment"
        tools:layout="@layout/fragment_cvi_display_settings" />
    <fragment
        android:id="@+id/presetsFragment"
        android:name="com.switch2go.aac.presets.PresetsFragment"
        android:label="PresetsFragment"
        tools:layout="@layout/fragment_presets">
        <action
            android:id="@+id/action_presetsFragment_to_keyboardFragment"
            app:destination="@id/keyboardFragment" />
        <action
            android:id="@+id/action_presetsFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_presetsFragment_to_addPhraseKeyboardFragment"
            app:destination="@id/addPhraseKeyboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/keyboardFragment"
        android:name="com.switch2go.aac.keyboard.KeyboardFragment"
        android:label="KeyboardFragment"
        tools:layout="@layout/fragment_keyboard">
        <action
            android:id="@+id/action_keyboardFragment_to_presetsFragment"
            app:destination="@id/presetsFragment"
            app:popUpTo="@id/presetsFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_keyboardFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/addToCategoryPickerFragment"
        android:name="com.switch2go.aac.presets.AddToCategoryPickerFragment"
        android:label="AddToCategoryPickerFragment" >
        <argument
            android:name="phraseText"
            app:argType="string" />
        <action
            android:id="@+id/action_addToCategoryPickerFragment_to_editCategoriesKeyboardFragment"
            app:destination="@id/editCategoriesKeyboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/editCategoryMenuFragment"
        android:name="com.switch2go.aac.settings.EditCategoryMenuFragment"
        android:label="fragment_edit_category_menu"
        tools:layout="@layout/fragment_edit_category_menu" >
        <argument
            android:name="category"
            app:argType="com.switch2go.aac.presets.Category" />
        <action
            android:id="@+id/action_editCategoryMenuFragment_to_editCategoryPhrasesFragment"
            app:destination="@id/editCategoryPhrasesFragment" />
        <action
            android:id="@+id/action_editCategoryMenuFragment_to_editCategoriesKeyboardFragment"
            app:destination="@id/editCategoriesKeyboardFragment" />
    </fragment>
</navigation>